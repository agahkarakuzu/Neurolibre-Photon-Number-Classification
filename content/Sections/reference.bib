@article{fukuda_titanium-based_2011,
author = {Daiji Fukuda and Go Fujii and Takayuki Numata and Kuniaki Amemiya and Akio Yoshizawa and Hidemi Tsuchida and Hidetoshi Fujino and Hiroyuki Ishii and Taro Itatani and Shuichiro Inoue and Tatsuya Zama},
journal = {Opt. Express},
keywords = {Optical design and fabrication; Quantum detectors; Absorption coefficient; Coupling efficiency; Detectors; Optical fibers; Photonic entanglement; Quantum communications},
number = {2},
pages = {870--875},
publisher = {Optica Publishing Group},
title = {Titanium-based transition-edge photon number resolving detector with 98\% detection efficiency with index-matched small-gap fiber coupling},
volume = {19},
month = {Jan},
year = {2011},
url = {https://opg.optica.org/oe/abstract.cfm?URI=oe-19-2-870},
doi = {10.1364/OE.19.000870},
abstract = {We have realized a high-detection-efficiency photon number resolving detector at an operating wavelength of about 850 nm. The detector consists of a titanium superconducting transition edge sensor in an optical cavity, which is directly coupled to an optical fiber using an approximately 300-nm gap. The gap reduces the sensitive area and heat capacity of the device, leading to high photon number resolution of 0.42 eV without sacrificing detection efficiency or signal response speed. Wavelength dependent efficiency in fiber-coupled devices, which is due to optical interference between the fiber and the device, is also decreased to less than 1\% in this configuration. The overall system detection efficiency is 98\%{\textpm}1\% at wavelengths of around 850 nm, which is the highest value ever reported in this wavelength range.},
}


@article{levine_algorithm_2012,
author = {Zachary H. Levine and Thomas Gerrits and Alan L. Migdall and Daniel V. Samarov and Brice Calkins and Adriana E. Lita and Sae Woo Nam},
journal = {J. Opt. Soc. Am. B},
keywords = {Quantum detectors; Radiometry; Detectors; Information processing; Photon counting; Quantum information; Resolution; Squeezed states},
number = {8},
pages = {2066--2073},
publisher = {Optica Publishing Group},
title = {Algorithm for finding clusters with a known distribution and its application to photon-number resolution using a superconducting transition-edge sensor},
volume = {29},
month = {Aug},
year = {2012},
url = {https://opg.optica.org/josab/abstract.cfm?URI=josab-29-8-2066},
doi = {10.1364/JOSAB.29.002066},
abstract = {Improving photon-number resolution of single-photon sensitive detectors is important for many applications, as is extending the range of such detectors. Here we seek improved resolution for a particular superconducting transition-edge sensor (TES) through better processing of the TES output waveforms. With that aim, two algorithms to extract number resolution from TES output waveforms are compared. The comparison is done by processing waveform data sets from a TES illuminated at nine illumination levels by a pulsed laser at 1550\&\#xA0;nm. The algorithms are used to sort the individual output waveforms and then create clusters associated with individual photon numbers. The first uses a dot product with the waveform mean (for each illumination level), while the second uses K-means clustering modified to include knowledge of the Poisson distribution. The first algorithm is shown to distinguish adjacent peaks associated with photon numbers up to 19, whereas the second algorithm distinguishes photon numbers up to 23, using the same data.},
}

