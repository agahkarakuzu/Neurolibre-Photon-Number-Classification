	langid = {english},
	file = {Takeda and Furusawa - 2019 - Toward large-scale fault-tolerant universal photon.pdf:C\:\\Users\\dalbe\\Zotero\\storage\\IVBFHNWT\\Takeda and Furusawa - 2019 - Toward large-scale fault-tolerant universal photon.pdf:application/pdf},
